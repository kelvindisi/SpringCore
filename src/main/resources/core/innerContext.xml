<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="kamba" class="org.devkiu.spring.core.bean.inner.Artist">
        <constructor-arg name="fullName" value="Kithungo" />
        <constructor-arg name="instrument" >
            <bean id="kithember" class="org.devkiu.spring.core.bean.inner.Instrument">
                <constructor-arg name="instrumentName" value="Kithembe" />
            </bean>
        </constructor-arg>
    </bean>
</beans>