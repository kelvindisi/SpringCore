<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="teacher" autowire="byName" class="org.devkiu.spring.autowiring.byNameXML.Teacher">
        <property name="fullName" value="Kelvin Ndisi" />
    </bean>
    <bean name="address" class="org.devkiu.spring.autowiring.byNameXML.Address">
        <property name="address" value="Muthini Estate" />
    </bean>
    <bean name="address2" class="org.devkiu.spring.autowiring.byNameXML.Address">
        <property name="address" value="Juja gate C" />
    </bean>
</beans>