<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="teacher" autowire="byType" class="org.devkiu.spring.autowiring.byTypeXML.Teacher">
        <property name="fullName" value="Abigail Wavinya" />
    </bean>
    <bean id="address" class="org.devkiu.spring.autowiring.byTypeXML.Address">
        <property name="address" value="Machakos Dallas" />
    </bean>
</beans>